/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "jdcloud,re-cp-02";
	model = "JD-Cloud RE-CP-02";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		sys_red {
			label = "power_2";
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
		};

		sys_green {
			label = "power_3";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
		};

		sys_blue {
			label = "power";
			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};
};

&spi0 {
	status = "okay";

	falsh@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partition@0 {
			label = "u-boot";
			reg = <0x00000 0x40000>;
		};

		partition@40000 {
			label = "u-boot-env";
			reg = <0x40000 0x10000>;
		};

		partition@50000 {
			label = "Factory";
			reg = <0x50000 0x40000>;
		};

		partition@90000 {
			label = "firmware";
			reg = <0x90000 0x01b70000> ;
		};

		partition@0x01c00000 {
			label = "ramfs";
			reg = <0x01c00000 0x0038000>;
		};

		partition@0x01f80000 {
			label = "noused";
			reg = <0x01f80000 0x00080000 >;
		};
	};
};

&pcie {
	status = "okay";
};

&sdhci {
	status = "okay";
};
